    <% include ../../../partials/head %>
    <body class="support">
        <% include ../../../partials/navbar %>
        <main>
            <div class="container">
                <div class="row">
                    <div class="col-12 text-center mt-5">
                        <h1><%= translation.SUPPORT_ELEMENT.FORM_SUPPORT.PAGE_TITLE_CPU %></h1>
                        <form class="text-center border border-light p-5" method="POST" action="send">
                            <h4 class="mb-4"><%= translation.SUPPORT_ELEMENT.FORM_SUPPORT.TYPE_OF_CPU %></h4>
                            <select class="browser-default custom-select mb-4" name="model">
                                <option disabled selected><%= translation.SUPPORT_ELEMENT.FORM_SUPPORT.CHOOSE_MODEL %></option>
                                <option>Howl 3</option>
                                <option>Howl 5</option>
                                <option>Howl 7</option>
                            </select>
                            <input type="text" class="form-control mb-4" name="serialNumber" placeholder="<%= translation.SUPPORT_ELEMENT.FORM_SUPPORT.SERIAL_NUMBER %>">
                            <h4 class="mb-4"><%= translation.SUPPORT_ELEMENT.FORM_SUPPORT.PROBLEM %></h4>
                            <div class="form-group">
                                <textarea class="form-control rounded-0" name="problem" rows="3" placeholder="<%= translation.SUPPORT_ELEMENT.FORM_SUPPORT.PROBLEM_EXPLAINATION %>"></textarea>
                            </div>
                            <h4 class="mb-4"><%= translation.SUPPORT_ELEMENT.FORM_SUPPORT.CONTACT %></h4>
                            <input type="email" class="form-control mb-4" name="email" placeholder="<%= translation.SUPPORT_ELEMENT.FORM_SUPPORT.EMAIL %>">
                            <button class="btn btn-info btn-block" type="submit">Envoyer</button>
                        </form>
                    </div>
                </div>
            </div>
        </main>
        <% include ../../../partials/footer %>
    </body>
</html>